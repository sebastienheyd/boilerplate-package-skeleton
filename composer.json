{
    "name": "~vendor/~package",
    "description": "~package_description",
    "license": "~license",
    "authors": [
        {
            "name": "~author_name",
            "email": "~author_email"
        }
    ],
    "keywords": ["Laravel", "Boilerplate", "Package"],
    "require": {
        "illuminate/support": "^5|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3|^4|^5|^6",
        "sempro/phpunit-pretty-print": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "~uc:vendor\\~uc:package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "~uc:vendor\\~uc:package\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "~uc:vendor\\~uc:package\\~uc:packageServiceProvider"
            ],
            "aliases": {
                "~uc:package": "~uc:vendor\\~uc:package\\Facades\\~uc:package"
            }
        }
    }
}

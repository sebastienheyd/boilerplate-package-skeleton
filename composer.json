{
    "name": ":vendor/:package",
    "description": ":package_description",
    "license": ":license",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email"
        }
    ],
    "keywords": ["Laravel"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            ":sc:vendor\\:sc:package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":sc:vendor\\:sc:package\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                ":sc:vendor\\:sc:package\\:sc:packageServiceProvider"
            ],
            "aliases": {
                ":sc:package": ":sc:vendor\\:sc:package\\Facades\\:sc:package"
            }
        }
    }
}
